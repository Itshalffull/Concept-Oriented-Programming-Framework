# AgentLoop run resolves the strategy string via PluginRegistry
# to find the appropriate strategy provider.

sync AgentLoopDispatchesToStrategy [eager]
when {
  AgentLoop/run: [agent: ?a; goal: ?g; context: ?c; strategy: ?strategy] => _
}
then {
  PluginRegistry/resolve: [plugin_type: "agent_strategy"; plugin_id: ?strategy]
}
