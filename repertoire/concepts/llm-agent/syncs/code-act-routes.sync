# PluginRegistry resolves "code_act" strategy to CodeActStrategy
# for code generation and sandbox execution.

sync CodeActRoutes [integration]
when {
  PluginRegistry/resolve: [plugin_type: "agent_strategy"; plugin_id: "code_act"] => _
}
then {
  CodeActStrategy/execute: [agent_ref: ?a; goal: ?g; context: ?c]
}
