  "invariants": [
    {
      "description": "invariant 1: after autoDiscover, map, apply behaves correctly",
      "setup": [
        {
          "action": "autoDiscover",
          "inputs": [
            {
              "name": "sourceSchema",
              "value": {
                "kind": "literal",
                "value": "external_post"
              }
            },
            {
              "name": "destSchema",
              "value": {
                "kind": "literal",
                "value": "Article"
              }
            }
          ],
          "expectedVariant": "ok",
          "expectedOutputs": [
            {
              "name": "mappingId",
              "value": {
                "kind": "literal",
                "value": "map-1"
              }
            },
            {
              "name": "suggestions",
              "value": {
                "kind": "literal",
                "value": "[{\"src\":\"title\",\"dest\":\"title\"}]"
              }
            }
          ]
        }
      ],
      "assertions": [
        {
          "action": "map",
          "inputs": [
            {
              "name": "mappingId",
              "value": {
                "kind": "literal",
                "value": "map-1"
              }
            },
            {
              "name": "sourceField",
              "value": {
                "kind": "literal",
                "value": "body_html"
              }
            },
            {
              "name": "destField",
              "value": {
                "kind": "literal",
                "value": "body"
              }
            },
            {
              "name": "transform",
              "value": {
                "kind": "literal",
                "value": "html_to_markdown"
              }
            }
          ],
          "expectedVariant": "ok",
          "expectedOutputs": []
        },
        {
          "action": "apply",
          "inputs": [
            {
              "name": "record",
              "value": {
                "kind": "literal",
                "value": "{\"title\":\"Hello\",\"body_html\":\"<p>World</p>\"}"
              }
            },
            {
              "name": "mappingId",
              "value": {
                "kind": "literal",
                "value": "map-1"
              }
            }
          ],
          "expectedVariant": "ok",
          "expectedOutputs": [
            {
              "name": "mapped",
              "value": {
                "kind": "literal",
                "value": "{\"title\":\"Hello\",\"body\":\"World\"}"
              }
            }
          ]
        }
      ],
