              "value": {
                "kind": "literal",
                "value": "import"
              }
            },
            {
              "name": "agent",
              "value": {
                "kind": "literal",
                "value": "system"
              }
            },
            {
              "name": "inputs",
              "value": {
                "kind": "literal",
                "value": ""
              }
            }
          ],
          "expectedVariant": "ok",
          "expectedOutputs": [
            {
              "name": "recordId",
              "value": {
                "kind": "literal",
                "value": "prov-batch-1"
              }
            }
          ]
        }
      ],
      "assertions": [
        {
          "action": "rollback",
          "inputs": [
            {
              "name": "batchId",
              "value": {
                "kind": "literal",
                "value": "batch-1"
              }
            }
          ],
          "expectedVariant": "ok",
          "expectedOutputs": [
            {
              "name": "rolled",
              "value": {
                "kind": "literal",
                "value": 1
              }
            }
          ]
        }
      ],
      "freeVariables": []
    }
