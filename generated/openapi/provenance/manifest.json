  "invariants": [
    {
      "description": "invariant 1: after record, trace behaves correctly",
      "setup": [
        {
          "action": "record",
          "inputs": [
            {
              "name": "entity",
              "value": {
                "kind": "literal",
                "value": "item-1"
              }
            },
            {
              "name": "activity",
              "value": {
                "kind": "literal",
                "value": "capture"
              }
            },
            {
              "name": "agent",
              "value": {
                "kind": "literal",
                "value": "system"
              }
            },
            {
              "name": "inputs",
              "value": {
                "kind": "literal",
                "value": ""
              }
            }
          ],
          "expectedVariant": "ok",
          "expectedOutputs": [
            {
              "name": "recordId",
              "value": {
                "kind": "literal",
                "value": "prov-1"
              }
            }
          ]
        }
      ],
      "assertions": [
        {
          "action": "trace",
          "inputs": [
            {
              "name": "entityId",
              "value": {
                "kind": "literal",
                "value": "item-1"
              }
            }
          ],
          "expectedVariant": "ok",
          "expectedOutputs": [
            {
              "name": "chain",
              "value": {
                "kind": "literal",
                "value": "[{\"activity\":\"capture\",\"agent\":\"system\"}]"
              }
            }
          ]
        }
      ],
      "freeVariables": []
    },
    {
      "description": "invariant 2: after record, rollback behaves correctly",
      "setup": [
        {
          "action": "record",
          "inputs": [
            {
              "name": "entity",
              "value": {
                "kind": "literal",
                "value": "item-1"
              }
            },
