          "expectedVariant": "ok",
          "expectedOutputs": [
            {
              "name": "valid",
              "value": {
                "kind": "literal",
                "value": "true"
              }
            },
            {
              "name": "score",
              "value": {
                "kind": "literal",
                "value": "0.95"
              }
            }
          ]
        }
      ],
