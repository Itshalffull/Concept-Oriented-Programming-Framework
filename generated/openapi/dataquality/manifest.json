            }
          ],
          "expectedVariant": "ok",
          "expectedOutputs": [
            {
              "name": "score",
              "value": {
                "kind": "literal",
                "value": "0.95"
              }
            },
            {
              "name": "violations",
              "value": {
                "kind": "literal",
                "value": "[]"
              }
            }
          ]
        }
      ],
