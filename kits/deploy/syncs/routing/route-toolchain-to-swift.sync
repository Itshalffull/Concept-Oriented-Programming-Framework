sync RouteToolchainToSwift [eager]
when {
  Toolchain/resolve: [ language: "swift"; platform: ?plat;
    versionConstraint: ?ver ]
    => [ tool: ?tool; version: ?v; path: ?path;
         capabilities: ?caps ]
}
then {
  SwiftToolchain/resolve: [ platform: ?plat;
    versionConstraint: ?ver ]
}
