# Check BuildCache before PulumiProvider generates â€” Section 3.8

sync CheckCacheBeforePulumi [eager]
when {
  IaC/emit: [ plan: ?plan; provider: "pulumi" ]
    => [ output: ?output; fileCount: ?count ]
}
then {
  BuildCache/check: [
    stepKey: ?plan;
    inputHash: ?plan;
    deterministic: false
  ]
}
