# Check BuildCache before TerraformProvider generates â€” Section 3.8

sync CheckCacheBeforeTerraform [eager]
when {
  IaC/emit: [ plan: ?plan; provider: "terraform" ]
    => [ output: ?output; fileCount: ?count ]
}
then {
  BuildCache/check: [
    stepKey: ?plan;
    inputHash: ?plan;
    deterministic: false
  ]
}
