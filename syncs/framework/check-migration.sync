// Uses deploy kit Migration concept (plan action with expand/contract pattern)
sync CheckMigrationOnRegister [eager]
when {
  Registry/register: [ uri: ?uri ] => [ concept: ?c ]
}
where {
  SchemaGen: { ?uri manifest: ?specVersion }
}
then {
  Migration/plan: [ concept: ?c; fromVersion: 0; toVersion: ?specVersion ]
}
