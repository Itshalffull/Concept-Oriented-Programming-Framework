# Route CoifComponentScaffoldGen file output through Emitter

sync EmitCoifComponentScaffoldFiles [eager]
  purpose { Write generated Clef Surface component scaffold through Emitter. }
when {
  CoifComponentScaffoldGen/generate: [ name: ?name ]
    => [ files: ?files ]
}
then {
  Emitter/writeBatch: [ files: ?files ]
}
