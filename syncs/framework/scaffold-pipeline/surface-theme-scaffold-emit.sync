# Route CoifThemeScaffoldGen file output through Emitter

sync EmitCoifThemeScaffoldFiles [eager]
  purpose { Write generated Clef Surface theme scaffold through Emitter. }
when {
  CoifThemeScaffoldGen/generate: [ name: ?name ]
    => [ files: ?files ]
}
then {
  Emitter/writeBatch: [ files: ?files ]
}
