# All targets for a suite complete triggers surface composition
# Architecture doc: Clef Bind, Sync Section 3.8

sync ComposeOnComplete [eager]
when {
  Generator/generate: [ plan: ?plan ]
    => [ plan: ?plan; filesGenerated: ?count ]
}
then {
  ApiSurface/compose: [ kit: ?kit; target: ?target; outputs: ?outputs ]
}
