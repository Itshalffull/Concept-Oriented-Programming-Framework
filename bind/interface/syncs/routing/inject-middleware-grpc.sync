# gRPC target complete triggers middleware resolution
# Architecture doc: Clef Bind, Sync Section 3.6

sync InjectMiddlewareGrpc [eager]
when {
  GrpcTarget/generate: [ projection: ?p ]
    => [ services: ?services; files: ?files ]
}
then {
  Middleware/resolve: [ traits: ?traits; target: "grpc" ]
}
